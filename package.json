    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "imageurl-base64": "^1.0.0",
    "mongoose": ">=5.7.5",
    "passport": "^0.4.0",
    "passport-coinbase": "^0.1.2",
